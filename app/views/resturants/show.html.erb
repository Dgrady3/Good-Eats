<h3>Title: <%=@resturant.name%></h3>

<h3>Address: <%=@resturant.address%></h3>

<h3>City: <%=@resturant.city%></h3>

<h3>State: <%=@resturant.state%></h3>

<h3>Zipcode: <%=@resturant.zipcode%></h3>

<h3>Category: <%=@resturant.category%></h3>

<h3>Description: <%=@resturant.description%></h3>

<br>

<div>
<h3>Reviews</h3>
<ul>
<% @resturant.reviews.each do |review| %>

    <p>Rating: <%=review.rating%><br>
    Description: <%=review.description%></p>
<%end%>
</ul>
</div>

<h5>Add a review below:</h5>
<%=link_to'Submit a review', new_resturant_review_path(@resturant) %> <br>
<%=link_to 'Back to resturant homepage', resturants_path %>
